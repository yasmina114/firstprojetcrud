pipeline {
    agent any
    stages {
        stage('Checkout GIT') {
            steps {
                checkout([
                    $class: 'GitSCM',
                    branches: [[name: 'main']],
                    userRemoteConfigs: [[url: 'https://github.com/yasmina114/firstprojetcrud.git']]
                ])
            }
        }

        stage('MVN CLEAN') {
            steps {
               withMaven(maven : 'apache-maven-3.9.0') {
              bat 'mvn clean compile'

           }
         }
        }





      }
    }