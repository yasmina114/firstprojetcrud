pipeline {
    agent any
    stages {
        stage('Checkout GIT') {
            steps {
                checkout([
                    $class: 'GitSCM',
                    branches: [[name: 'main']],
                    userRemoteConfigs: [[url: 'https://github.com/yasmina114/firstprojetcrud.git']]
                ])
            }
        }

       stage('MVN CLEAN') {
                   steps {
                      withMaven() {
                     bat 'mvn clean compile'

                  }
                }
               }

       stage('MVN COMPILE') {
                           steps {
                               bat 'mvn compile'
                           }
                       }

                       stage('MVN PACKAGE') {
                           steps {
                               bat 'mvn package'
                           }
                       }

                       stage('MVN TEST') {
                           steps {
                               bat 'mvn test'
                           }
                       }

                           stage('MVN SONARQUBE') {
                                   steps {
                                       bat 'mvn clean verify sonar:sonar \
                                              -Dsonar.projectKey=yas \
                                              -Dsonar.host.url=http://localhost:9002 \
                                              -Dsonar.login=sqp_ce4ded49f98fbc902d2beda2f90ad4c5034d454c'
                                   }
                               }



                                       stage('Docker login') {

                                                       steps {
                                                         bat 'echo "login Docker ...."'
                                                         bat'docker login -u yasminaa -p y1a2s3m4i5n6'
                                                                    }  }
                                       stage('Docker push') {

                                                       steps {
                                                           bat 'echo "Docker is pushing ...."'
                                                       	bat 'docker push yasminaa/firstprojetcrud-0.0.1:latest'
                                                                    }  }




      }
    }
